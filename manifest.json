{
  "manifest_version": 2,
  "name": "Unemployment_Front_End",
  "version": "2.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "history"
  ],
  "content_scripts": [
    {
       "matches": ["http://*/*","https://*/*"],
      "js": ["jquery-3.5.0.min.js", "popup.js"]
    }
  ]

}
